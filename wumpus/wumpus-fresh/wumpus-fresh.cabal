build-type:         Simple
stability:          unstable
cabal-version:      >= 1.2

extra-source-files:
  CHANGES,
  LICENSE,
  demo/Demo01.hs


library
  hs-source-dirs:     src
  build-depends:      base            <  5, 
                      containers      >= 0.3      && <= 0.4, 
                      time            >= 1.1.3    && < 1.2,
                      vector-space    >= 0.6      && < 1.0,
                      algebra         >= 0.0.0.1
                        
  exposed-modules:
    Wumpus.Fresh.BoundingBox,
    Wumpus.Fresh.Colour,
    Wumpus.Fresh.FormatCombinators,
    Wumpus.Fresh.FontSize,
    Wumpus.Fresh.FreshIR,
    Wumpus.Fresh.Geometry,
    Wumpus.Fresh.GraphicsState,
    Wumpus.Fresh.OneList,
    Wumpus.Fresh.PostScript,
    Wumpus.Fresh.PostScriptDoc,
    Wumpus.Fresh.PtSize,
    Wumpus.Fresh.SVG,
    Wumpus.Fresh.SVGDoc,
    Wumpus.Fresh.TextEncoder,
    Wumpus.Fresh.TextInternal,
    Wumpus.Fresh.TextLatin1,
    Wumpus.Fresh.Utils


  other-modules:

