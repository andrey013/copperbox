-- HMinCaml.KNormal.KNormalSynDEFS

DATA Expr 
  | Unit
  | Int         val       : {Int}
  | Float       val       : {Float}
  | Neg         x         : {Id}
  | Add         x         : {Id}  
                y         : {Id}
  | Sub         x         : {Id}  
                y         : {Id}
  | FNeg        x         : {Id}
  | FAdd        x         : {Id}  
                y         : {Id}
  | FSub        x         : {Id}  
                y         : {Id}
  | FMul        x         : {Id}  
                y         : {Id}
  | FDiv        x         : {Id}  
                y         : {Id}
  | IfEq        x         : {Id}  
                y         : {Id}
                texpr     : Expr  
                eexpr     : Expr
  | IfLE        x         : {Id}  
                y         : {Id}
                texpr     : Expr  
                eexpr     : Expr
  | Let         tyid      : TypeId 
                sub       : Expr  
                body      : Expr
  | Var         ref       : {Id}
  | LetRec      fundef    : Fundef  
                body      : Expr
  | App         ref       : {Id}
                args      : {[Id]}
  | Tuple       refs      : {[Id]}
  | LetTuple    tyids     : TypeIds 
                ref       : {Id}  
                body      : Expr
  | Get         aref      : {Id}  
                iref      : {Id}
  | Put         aref      : {Id}  
                iref      : {Id}
                vref      : {Id}
  | ExtArray    ref       : {Id}
  | ExtFunApp   nref      : {Id}    
                args      : {[Id]}

  
DATA Fundef 
  | Fundef      name      : TypeId
                args      : TypeIds
                body      : Expr 

