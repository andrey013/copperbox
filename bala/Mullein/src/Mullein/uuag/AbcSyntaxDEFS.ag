DATA Element
  | Note        pitch   : {Pitch} 
                dmult   : {Multiplier}                 
  | Rest        dmult   : {Multiplier}
  | Spacer      dmult   : {Multiplier}
  | Chord       pitches : Pitches 
                dmult   : {Multiplier}         
  | GraceNotes  graces  : GraceNotes                             
  | Nplet       nmult   : {Int} 
                pitches : Pitches

TYPE Elements   = [Element]
TYPE GraceNotes = [GraceNote]
TYPE Pitches    = [Pitch]        